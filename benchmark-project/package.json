{
  "scripts": {
    "bench": "node ./benchmark.js",
    "build-webpack-prod": "webpack --mode production --output dist/main.webpack.prod.js",
    "build-webpack-dev": "webpack --mode development --output dist/main.webpack.dev.js",
    "build-rollup": "rollup -c",
    "build-browserify": "NODE_ENV=production browserify src/index.cjs -o dist/main.browserify.js",
    "build-fusebox": "node fuse",
    "build-percel": "NODE_ENV=production parcel build src/index.js --out-dir dist/percel --no-minify --no-cache"
  },
  "devDependencies": {
    "browserify": "^16.1.1",
    "fuse-box": "^3.1.3",
    "parcel-bundler": "^1.6.2",
    "rollup": "^0.56.5",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-replace": "^2.0.0",
    "typescript": "^2.7.2",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rxjs": "^5.5.6",
    "three": "^0.90.0"
  },
  "private": true
}
